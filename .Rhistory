library(tidyverse)
library(palmerpenguins)
ggplot(data = penguins,
aes(x = bill_length_mm,
y = body_mass_g,
color = species)) +
geom_point(size = 3, alpha = 0.7) +
labs(
title = "펭귄 종별 부리 길이와 체중 관계",
x = "부리 길이 (mm)",
y = "체중 (g)",
color = "종"
) +
theme_minimal()
